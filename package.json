{
  "name": "@trunkery/typescript-codegen",
  "version": "3.3.3",
  "description": "Typescript code generator for GraphQL",
  "bin": {
    "trunkery-typescript-codegen": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "check-format": "import-sort -l 'src/**/*.{ts,tsx}' && prettier -c 'src/**/*.{ts,tsx}'",
    "format": "import-sort --write 'src/**/*.{ts,tsx}' && prettier -w 'src/**/*.{ts,tsx}'",
    "build": "tsc"
  },
  "author": "nsf <no.smile.face@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.36",
    "@types/prompts": "^2.0.14",
    "@types/superagent": "^4.1.15",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-renke": "^6.0.0",
    "prettier": "^3.0.3",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.3.0",
    "glob": "^8.0.3",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "prompts": "^2.4.2",
    "superagent": "^7.1.3",
    "superstruct": "^0.15.4"
  },
  "importSort": {
    ".js, .jsx, .es6, .es, .mjs, .ts, .tsx": {
      "parser": "typescript",
      "style": "renke"
    }
  }
}